@using TuProyecto.Models
@model CalculoModel

<h2>Calculadora de Caída Libre</h2>

<form method="post" asp-action="Calcular">
    <label for="tiempo">Ingrese el tiempo en segundos:</label>
    <input type="number" step="0.01" name="tiempo" required />
    <button type="submit">Calcular</button>
</form>
